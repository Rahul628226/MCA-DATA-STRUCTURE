#include<stdio.h>
int main()
{
	int i,a[10],n,pos,ele;
	printf("enter array size");
	scanf("%d",&n);
	printf("enter array elements\n");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("Enter the position=");
	scanf("%d",&pos);
	
	
	printf("Enter elements=");
	scanf("%d",&ele);
	for(i=n;i>pos-1;i--)
	  {
		a[i]=a[i-1];
	  }
	a[pos-1]=ele;
	printf("\n h=%d\n",a[n]);

	for(i=0;i<=n;i++)
	  {
		printf("%d\n",a[i]);
	  }
      
	return 0;
}
	
